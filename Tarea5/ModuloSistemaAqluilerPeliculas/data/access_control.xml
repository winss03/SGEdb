<odoo>
    <!-- Acceso a actores -->
    <record id="access_alquiler_actor" model="ir.model.access">
        <field name="name">Acceso actores</field>
        <field name="model_id" ref="alquiler.actor"/>  <!-- Correcto: 'alquiler.actor' -->
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <!-- Acceso a categorías -->
